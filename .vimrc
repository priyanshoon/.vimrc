syntax on
set tabstop=4
set shiftwidth=4
set expandtab
set number
set relativenumber
set cindent
colorscheme pablo
set autoindent
set smartindent
set smarttab
set guifont=*
highlight Comment ctermfg=green
set encoding=utf-8
filetype plugin indent on
set backspace=indent,eol,start
set clipboard=unnamedplus
hi Normal guibg=NONE ctermbg=NONE
set mouse=a
